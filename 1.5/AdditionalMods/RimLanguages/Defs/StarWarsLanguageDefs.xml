	<!-- the big two -->
	<FactionLanguages.DialectDef>
		<defName>guy762_SWLanguageDialect_galacticbasic</defName>
		<label>Galactic Basic</label>
		<description>The official language of the Republic, used as the primary trade language from the Deep Core all the way to the Mid Rim.</description>
		<nameMaker>guy762_NamerLanguageDialect_galacticbasic</nameMaker>
		<commonality>10</commonality>
		<learningDifficulty>1</learningDifficulty>
	</FactionLanguages.DialectDef>
	<RulePackDef>
		<defName>guy762_NamerLanguageDialect_galacticbasic</defName>
		<rulePack>
			<rulesStrings>
				<li>r_name->Galactic Basic</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
	
	
	
	
	
	<FactionLanguages.DialectDef>
		<defName>guy762_SWLanguageDialect_huttese</defName>
		<label>Huttese</label>
		<description>The most common trade language in the Outer Rim and the official language of the Hutt Kajidics.</description>
		<nameMaker>guy762_NamerLanguageDialect_huttese</nameMaker>
		<commonality>15</commonality>
		<learningDifficulty>1</learningDifficulty>
	</FactionLanguages.DialectDef>
	<RulePackDef>

	<FactionLanguages.DialectDef> <!-- NOT LINKED YET -->
		<defName>guy762_SWLanguageDialect_droidbinary</defName>
		<label>Binary</label>
		<description>A highly intricate form of communication utilized by droids. It is communicated primarily through tones, beeps, and warbles of varying pitches, frequencies, and amplitudes. It is very difficult (but not impossible) for sentient beings to learn how to detect the subtle tonal changes used.</description>
		<nameMaker>guy762_NamerLanguageDialect_droidbinary</nameMaker>
		<commonality>1</commonality>
		<learningDifficulty>4</learningDifficulty>
	</FactionLanguages.DialectDef>

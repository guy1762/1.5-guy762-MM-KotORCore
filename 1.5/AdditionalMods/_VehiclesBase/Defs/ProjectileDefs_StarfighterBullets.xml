	<ThingDef ParentName="BaseBullet">
		<defName>KotORSFBullet_missileHX</defName>
		<label>high-ex missile</label>
		<graphicData>
			<texPath>Weapons/Projectiles/missile_orange</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>TransparentPostLight</shaderType>
			<drawSize>2</drawSize>
		</graphicData>
		<projectile>
			<damageDef>guy762_GrenadeDamage_frag</damageDef>
			<damageAmountBase>25</damageAmountBase>
			<armorPenetrationBase>0.375</armorPenetrationBase>
			<speed>100</speed>
			<explosionRadius>3</explosionRadius>
			<soundAmbient>kotorsound_missileflying</soundAmbient>
		</projectile>
		<thingClass>Projectile_Explosive</thingClass>
		<comps>
			<li Class="VFECore.CompProperties_HomingProjectile">
				<homingDistanceFractionPassed>0.7</homingDistanceFractionPassed>
				<homingCorrectionTickRate>5</homingCorrectionTickRate>
				<initialDispersionFromTarget>2.5</initialDispersionFromTarget>
			</li>
		</comps>
	</ThingDef>
	

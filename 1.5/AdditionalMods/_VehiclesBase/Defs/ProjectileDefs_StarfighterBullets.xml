		</graphicData>
		<!-- <thingClass>taranchuk_flightcombat.Bomb</thingClass> -->
		<projectile>
		  <damageDef>guy762_ExplosiveDamage_AntiVehicleEnergy</damageDef>
		  <damageAmountBase>45</damageAmountBase>
		  <armorPenetrationBase>0.25</armorPenetrationBase>
